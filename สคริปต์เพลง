-- [ ‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô ] --
local function ShowData(sid)
    local name, creator = GetInfo(sid)
    if name then
        InfoTxt.Text = "‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏û‡∏•‡∏á: "..name.."\n‡∏Ñ‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á: "..creator.."\n‡πÑ‡∏≠‡∏î‡∏µ: "..sid
        AlbumArt.Image = "rbxthumb://type=Asset&id="..sid.."&w=150&h=150"
        IDShow.Text = sid
    end
end

CopyBtn.MouseButton1Click:Connect(function()
    if IDShow.Text ~= "" then setclipboard(IDShow.Text) end
end)

ResetBtn.MouseButton1Click:Connect(function()
    IDShow.Text = ""; InfoTxt.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÅ‡∏•‡πâ‡∏ß..."; AlbumArt.Image = "rbxassetid://13847421375"
end)

local function RefreshPlayers()
    for _, v in pairs(ListScroll:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end
    for _, p in pairs(Players:GetPlayers()) do
        local b = Instance.new("TextButton", ListScroll)
        b.Size = UDim2.new(1, -5, 0, 30); b.BackgroundColor3 = Color3.fromRGB(20, 20, 20); b.TextColor3 = Color3.new(1,1,1); b.TextSize = 10; b.Text = "  üë§ "..p.DisplayName; b.TextXAlignment = 0; b.Font = Enum.Font.Gotham; Instance.new("UICorner", b)
        b.MouseButton1Click:Connect(function()
            local sid = nil
            if p.Character then for _, sound in pairs(p.Character:GetDescendants()) do if sound:IsA("Sound") and sound.Playing and sound.SoundId:match("%d+") then sid = sound.SoundId:match("%d+") end end end
            if sid and tonumber(sid) > 100 then ShowData(sid) else InfoTxt.Text = "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô: "..p.DisplayName.."\n‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏û‡∏•‡∏á"; AlbumArt.Image = "rbxassetid://13847421375"; IDShow.Text = "" end
        end)
    end
end

ScanBtn.MouseButton1Click:Connect(function()
    for _, v in pairs(ScanScroll:GetChildren()) do if v:IsA("Frame") then v:Destroy() end end
    for _, p in pairs(Players:GetPlayers()) do
        local sid = nil
        if p.Character then for _, sound in pairs(p.Character:GetDescendants()) do if sound:IsA("Sound") and sound.Playing and sound.SoundId:match("%d+") then sid = sound.SoundId:match("%d+") end end end
        if sid and tonumber(sid) > 100 then
            local name, creator = GetInfo(sid)
            if name then
                local f = Instance.new("Frame", ScanScroll); f.Size = UDim2.new(1, -10, 0, 45); f.BackgroundColor3 = Color3.fromRGB(20, 20, 20); Instance.new("UICorner", f)
                local img = Instance.new("ImageLabel", f); img.Size = UDim2.new(0, 35, 0, 35); img.Position = UDim2.new(0, 5, 0.5, -17.5); img.Image = "rbxthumb://type=Asset&id="..sid.."&w=150&h=150"; Instance.new("UICorner", img)
                local t = Instance.new("TextLabel", f); t.Size = UDim2.new(0.6, 0, 1, 0); t.Position = UDim2.new(0, 45, 0, 0); t.Text = "‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÇ‡∏î‡∏¢: "..creator.."\n‡πÄ‡∏û‡∏•‡∏á: "..name; t.TextColor3 = Color3.new(1,1,1); t.TextSize = 9; t.BackgroundTransparency = 1; t.TextXAlignment = 0; t.Font = Enum.Font.GothamBold
                local c = Instance.new("TextButton", f); c.Size = UDim2.new(0.2, 0, 0.6, 0); c.Position = UDim2.new(0.78, 0, 0.2, 0); c.Text = "‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å"; c.BackgroundColor3 = Color3.new(1,1,1); c.TextColor3 = Color3.new(0,0,0); c.TextSize = 9; c.Font = Enum.Font.GothamBold; Instance.new("UICorner", c)
                c.MouseButton1Click:Connect(function() setclipboard(sid) end)
            end
        end
    end
end)

-- [ ‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏•‡∏±‡∏ö‡∏´‡∏ô‡πâ‡∏≤ ] --
Tab1Btn.MouseButton1Click:Connect(function() Page1.Visible = true; Page2.Visible = false; Tab1Btn.BackgroundColor3 = Color3.new(1,1,1); Tab1Btn.TextColor3 = Color3.new(0,0,0); Tab2Btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30); Tab2Btn.TextColor3 = Color3.new(1,1,1) end)
Tab2Btn.MouseButton1Click:Connect(function() Page1.Visible = false; Page2.Visible = true; Tab2Btn.BackgroundColor3 = Color3.new(1,1,1); Tab2Btn.TextColor3 = Color3.new(0,0,0); Tab1Btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30); Tab1Btn.TextColor3 = Color3.new(1,1,1) end)

-- [ ‡∏£‡∏∞‡∏ö‡∏ö RGB ] --
RunService.RenderStepped:Connect(function() local c = Color3.fromHSV(tick() * 0.4 % 1, 0.7, 1); tStroke.Color = c; mStroke.Color = c end)
RefreshPlayers(); Players.PlayerAdded:Connect(RefreshPlayers); Players.PlayerRemoving:Connect(RefreshPlayers)
